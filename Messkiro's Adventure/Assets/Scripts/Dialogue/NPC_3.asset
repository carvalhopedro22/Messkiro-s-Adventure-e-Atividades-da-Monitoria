      spanish: 
  - actorName: Alfred
    profile: {fileID: 21300000, guid: 903aafe204f21644a8efa8cef3d68bbb, type: 3}
    sentece:
      portuguese: "Blocos B\xE1sicos e Grafos de Fluxo:"
      english: 
      spanish: 
  - actorName: Alfred
    profile: {fileID: 21300000, guid: 903aafe204f21644a8efa8cef3d68bbb, type: 3}
    sentece:
      portuguese: "Representa\xE7\xE3o do c\xF3digo intermedi\xE1rio na forma de
        um grafo."
      english: 
      spanish: 
  - actorName: Alfred
    profile: {fileID: 21300000, guid: 903aafe204f21644a8efa8cef3d68bbb, type: 3}
    sentece:
      portuguese: "No grafo teremos os blocos b\xE1sicos como n\xF3s e as arestas
